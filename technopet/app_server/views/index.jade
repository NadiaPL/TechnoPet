extends layout

block content
  .row.page-header
    .col-lg-12
      h1= title
  .row
    .col-xs-12.col-md-6
      form.form-horizontal(action="/location", method="get", role="form")
        .form-group
          label.col-xs-10.col-sm-2.control-label(for="abrir") Abrir
          .col-xs-12.col-sm-10
            input#name.form-control(name="abrir")
        .form-group
          label.col-xs-10.col-sm-2.control-label(for="Cerrar") Cerrar
          .col-xs-12.col-sm-10
            input#name.form-control(name="Cerrar")
        .form-group
          label.col-sm-2.control-label(for="Automatico") Automatico          
        button.btn.btn-default.pull-right Activar
        button.btn.btn-default.pull-right Desactivar
    .col-xs-12.col-md-4